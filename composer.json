{
    "name": "carsguide/error-handler",
    "description": "Error and exception handler",
    "keywords": [
        "Error",
        "Exception"
    ],
    "homepage": "https://github.com/carsguide/error-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Yama Zhang",
            "email": "yama.zhang@carsguide.com.au"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/lumen-framework": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/cache": "^7.0",
        "illuminate/container": "^7.0",
        "illuminate/log": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Carsguide\\Exceptions\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carsguide\\Tests\\": "tests"
        }
    }
}
